---
import AdminApp from "@/components/react/admin/AdminApp";
import Layout from "@/layouts/Layout.astro";

export const prerender = false;
Astro.response.headers.set("Cache-Control", "no-store, max-age=0, must-revalidate");
---

<Layout
  title="Admin panel"
  description="Управление статьями: создание, редактирование и публикация"
>
  <section class="mx-auto w-full max-w-7xl">
    <AdminApp client:load />
  </section>
</Layout>
